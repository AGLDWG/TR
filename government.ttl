augov:Contributor
  rdf:type owl:Class ;
  rdfs:comment "A Contributor is the Role of contributing in one form or another as an Agent to a Dataset."@en ;
  rdfs:subClassOf augov:Role ;
.
augov:Custodian
  rdf:type owl:Class ;
  rdfs:comment "A Custodian is the Role of being the responsible Agent that looks after a Dataset."@en ;
  rdfs:subClassOf augov:Role ;
.
augov:Daily
  rdf:type dct:Frequency ;
  rdf:type owl:NamedIndividual ;
.
augov:Dataset
  rdf:type owl:Class ;
  rdfs:comment "A Dataset is a collection of resources, published or curated by an agent, and openly available for download in one or more formats."@en ;
  rdfs:label "Dataset"@en ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty dct:license ;
      owl:someValuesFrom dct:LicenseDocument ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty augov:publishDate ;
      owl:someValuesFrom rdfs:Literal ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty augov:title ;
      owl:someValuesFrom rdfs:Literal ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty augov:updateDate ;
      owl:someValuesFrom rdfs:Literal ;
    ] ;
  owl:equivalentClass <http://www.w3.org/ns/dcat#Dataset> ;
.
augov:DatasetChangeEvent
  rdf:type owl:Class ;
  rdfs:comment "A Dataset Change Event is a description of an event that happens at a given point in time and that results in any type of change to the Dataset."@en ;
  rdfs:label "Dataset Change Event"@en ;
  rdfs:subClassOf prov:InstantaneousEvent ;
.
augov:DatasetPublishingEvent
  rdf:type owl:Class ;
  rdfs:comment "A Dataset Publishing Event is a description of an event that happens at a given point in time and that results in the publishing of a Dataset on data.gov.au."@en ;
  rdfs:label "Dataset Publishing Event"@en ;
  rdfs:subClassOf augov:DatasetChangeEvent ;
.
augov:Distribution
