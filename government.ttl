  rdfs:label "Government Agency"@en ;
  rdfs:seeAlso "http://recordsearch.naa.gov.au/manual/Provenance/AgencyDefinition.htm#DEFINITION"@en ;
  rdfs:subClassOf augov:Organisation ;
.
augov:Jurisdiction
  rdf:type owl:Class ;
