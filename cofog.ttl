  terms:scopeoutScopeNote "public health service laboratories (07.4.0);  laboratories engaged in determining the causes of disease (07.5.0)."@en ;
  rdfs:label "Paramedical services  (IS)" ;
  skos:broader <http://unstats.un.org/unsd/cr/references/cofog/version1/07-2> ;
  skos:inScheme cofog: ;
